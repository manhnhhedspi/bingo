function changeStyle(id){
	var x = document.getElementById(id);
	x.style.backgroundImage = "url(<%= image_path('circle_green.png') %>)";
	x.style.width = "40px";
	x.style.height = "40px";
	x.style.border = "0px";
	x.style.backgroundRepeat = "no-repeat";
	x.style.textAlign = "center";
}

function changeBackground(element) {
	element.style.backgroundImage = "url(<%= image_path('circle_green.png') %>)";
	element.style.backgroundSize = "40px 40px";
}

function reachOnclick () {
	document.getElementById("reach").disabled = false;
}

function bingoOnclick () {
	document.getElementById("bingo").disabled = false;
}
